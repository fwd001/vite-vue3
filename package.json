{
  "name": "vite-vue3",
  "version": "2.0.1",
  "private": true,
  "description": "web前端框架",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --mode dev",
    "ts": "vue-tsc --noEmit",
    "build": "npm run ts && vite build",
    "bud": "vite build",
    "upload": "rm -f dist.zip && zip -r dist.zip ./dist && scp ./dist.zip root@192.168.240.169:/home/project/fe && rm -f dist.zip",
    "deploy": "ssh root@192.168.240.169 'cd /home/project/fe ; sh i.sh'",
    "re": "npm run build && npm run upload && npm run deploy",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx,.vue,.js,.jsx --fix src",
    "prettier": "prettier . --write",
    "prepare": "husky install",
    "up": "pnpm update '!typescript' '!vue-tsc'  --latest"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@antv/g6": "^4.8.21",
    "@tinymce/tinymce-vue": "^5.1.1",
    "@vueuse/core": "^10.4.1",
    "ant-design-vue": "^4.0.1",
    "axios": "^1.5.0",
    "colord": "^2.9.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "number-precision": "^1.6.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "qs": "^6.11.2",
    "ua-parser-js": "1.0.35",
    "vue": "^3.3.4",
    "vue-request": "^2.0.3",
    "vue-router": "^4.2.4",
    "xgplayer": "^3.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.8",
    "@types/mockjs": "^1.0.7",
    "@types/node": "20.5.7",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@unocss/preset-rem-to-px": "^0.55.3",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "cz-git": "^1.7.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.23.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "regenerator-runtime": "^0.14.0",
    "terser": "^5.19.3",
    "typescript": "5.0.2",
    "unocss": "^0.55.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.0",
    "vue-tsc": "1.8.0"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "npm run prettier",
      "npm run lint"
    ],
    "*.{html,css,less}": [
      "npm run prettier"
    ]
  },
  "target": "web",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
