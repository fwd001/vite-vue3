{
  "name": "vite-vue3",
  "version": "1.0.0",
  "private": true,
  "description": "web前端框架",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --mode dev",
    "build": "vue-tsc --noEmit && vite build",
    "bud": "vite build",
    "upload": "rm -f dist.zip && zip -r dist.zip ./dist && scp ./dist.zip root@192.168.240.169:/home/project/epcms-fe && rm -f dist.zip",
    "r": "npm run build && npm run upload",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx,.vue,.js,.jsx --fix src",
    "prettier": "prettier . --write",
    "prepare": "husky install",
    "update": "pnpm update '!nprogress' --latest"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@antv/g6": "^4.8.17",
    "@tinymce/tinymce-vue": "^5.1.0",
    "@vueuse/core": "^10.1.2",
    "ant-design-vue": "^3.2.20",
    "axios": "^1.4.0",
    "colord": "^2.9.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.8",
    "echarts": "^5.4.2",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "nprogress": "~1.0.0-1",
    "number-precision": "^1.6.0",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qs": "^6.11.2",
    "ua-parser-js": "1.0.35",
    "vue": "^3.3.4",
    "vue-request": "^2.0.3",
    "vue-router": "^4.2.2",
    "vuedraggable": "^2.24.3",
    "xgplayer": "^3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/mockjs": "^1.0.7",
    "@types/node": "20.3.1",
    "@types/qs": "^6.9.7",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@unocss/preset-rem-to-px": "^0.53.1",
    "@vitejs/plugin-legacy": "^4.0.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "cz-git": "^1.6.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.11",
    "terser": "^5.18.0",
    "typescript": "5.0.4",
    "unocss": "^0.53.1",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.0",
    "vue-tsc": "^1.6.5"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ],
    "*.{html,css,less}": [
      "npm run prettier",
      "git add"
    ]
  },
  "target": "web",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
